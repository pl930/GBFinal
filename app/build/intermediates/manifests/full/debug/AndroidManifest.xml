<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.assignments.koorong.gym_buddy_alpha_"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <service
        android:name="com.amazonaws.mobileconnectors.s3.transferutility.TransferService"
        android:enabled="true" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.Dark" >
        <activity
            android:name="com.assignments.koorong.gym_buddy_alpha_.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.assignments.koorong.gym_buddy_alpha_.LoginActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="com.assignments.koorong.gym_buddy_alpha_.SignupActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="com.assignments.koorong.gym_buddy_alpha_.MatchActivity"
            android:theme="@style/AppTheme.Dark" />
        <!--
        <activity
            android:name=".MatchActivity"
            android:label="@string/title_activity_match"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        -->
    </application>

</manifest>